{
  "name": "ackodrive",
  "version": "0.1.0",
  "private": true,
  "main": "webpack.config.js",
  "scripts": {
    "start": "webpack",
    "build": "NODE_ENV=production webpack --progress",
    "dev": "npm-run-all --parallel server client",
    "server": "nodemon index.js",
    "client": "webpack --watch --progress",
    "linter": "eslint src"
  },
  "dependencies": {
    "axios": "^0.18.0",
